{
    "name": "startuptask",
    "displayName": "StartupTask",
    "description": "",
    "version": "0.0.1",
    "publisher": "dydFreeman",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "StartupTask configuration",
            "properties": {
                "startuptask.taskList": {
                    "type": "array",
                    "default": [{"command": "", "args": ""}],
                    "description": "List of tasks to be executed on startup"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}