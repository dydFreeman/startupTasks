{
    "name": "startuptask",
    "displayName": "StartupTask",
    "description": "",
    "version": "0.1.4",
    "publisher": "dydFreeman",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "StartupTask configuration",
            "properties": {
                "startuptask.taskList": {
                    "type": "array",
                    "default": [
                        {
                            "command": "",
                            "args": ""
                        }
                    ],
                    "description": "List of tasks to be executed on startup"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.39",
        "@types/node": "^6.0.41",
        "gulp": "^3.9.1",
        "gulp-bump": "^2.1.0",
        "gulp-git": "^2.4.1",
        "gulp-inject-string": "^1.1.0",
        "gulp-shell": "^0.5.2",
        "gulp-tag-version": "^1.3.0",
        "gulp-tslint": "^7.0.1",
        "gulp-typings": "^2.0.0",
        "merge-stream": "^1.0.0",
        "mocha": "^3.2.0",
        "tslint": "^4.3.1",
        "typescript": "^2.3.2",
        "vscode": "^1.0.5"
    }
}